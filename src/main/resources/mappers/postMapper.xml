<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lhcgram.post.dao.PostDAO">
	<insert id="postCreate" parameterType="map">
		INSERT INTO `post`
		(
			`userId`
			,`userName`
			,`content`
			,`imagePath`
			,`createdAt`
			,`updatedAt`
		)
		VALUES
		(
			#{userId}
			,#{userLoginId}
			,#{content}
			,#{imagePath}
			,NOW()
			,NOW()
		)
	</insert>
</mapper>